<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFECC2"
    tools:context=".MainActivity_ZZZero">


    <ImageButton
        android:id="@+id/button4"
        android:layout_width="181dp"
        android:layout_height="212dp"
        android:background="@drawable/sample_0"
        android:text="餅乾"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img" />

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="145dp"
        android:layout_height="158dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/sample_5"
        android:text="麵包"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img" />

    <ImageView
        android:id="@+id/img" android:layout_width="356dp"
        android:layout_height="383dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bgc01" />
</androidx.constraintlayout.widget.ConstraintLayout>